CREATE TABLE meekbot.stream_reward_redeem (
active_ind BOOLEAN NOT NULL DEFAULT TRUE
,stream_reward_redeem_id BIGSERIAL NOT NULL PRIMARY KEY
,stream_id BIGINT NOT NULL
,reward_name text COLLATE pg_catalog."default" NOT NULL
,reward_cost BIGINT NOT NULL
,redeem_total BIGINT NOT NULL
,redeem_delay INT NOT NULL
,redeem_delay_unit_cd BIGINT NOT NULL
,redeem_stream_reltn_cd BIGINT NOT NULL
,create_dt_tm timestamptz NOT NULL
,updt_dt_tm timestamptz NOT NULL DEFAULT now()
)

WITH (
    OIDS = FALSE
) TABLESPACE meekbot;

ALTER TABLE meekbot.stream_reward_redeem    OWNER to "Mike";

COMMENT ON TABLE meekbot.stream_reward_redeem
 IS 'Contains the different rewards for a stream as well as cost/delay/permissions';

CREATE INDEX xie1_stream_reward_redeem
 ON meekbot.stream_reward_redeem USING btree
 (stream_reward_redeem_id)
 TABLESPACE meekbot;

CREATE INDEX xie2_stream_reward_redeem
 ON meekbot.stream_reward_redeem USING btree
 (stream_id)
 TABLESPACE meekbot;